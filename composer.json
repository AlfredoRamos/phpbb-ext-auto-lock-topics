{
	"name": "alfredoramos/autolocktopics",
	"type": "phpbb-extension",
	"description": "Auto-lock Topics extension for phpBB",
	"homepage": "https://github.com/AlfredoRamos/phpbb-ext-auto-lock-topics",
	"version": "1.2.0",
	"time": "2020-01-06",
	"keywords": [
		"phpbb",
		"extension",
		"auto-lock",
		"topics",
		"cron"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Alfredo Ramos",
			"email": "alfredo.ramos@skiff.com",
			"homepage": "https://alfredoramos.mx/",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": "^7.1.3 || ^8.0.0",
		"composer/installers": "^1.6.0"
	},
	"require-dev": {
		"phing/phing": "^2.16.0",
		"pear/archive_tar": "^1.4.0"
	},
	"extra": {
		"display-name": "Auto-lock Topics",
		"soft-require": {
			"phpbb/phpbb": "~3.3.0"
		},
		"version-check": {
			"host": "alfredoramos.mx",
			"directory": "/phpbb-extensions/versions",
			"filename": "auto-lock-topics.json",
			"ssl": true
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	}
}